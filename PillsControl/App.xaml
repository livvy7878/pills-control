<Application x:Class="PillsControl.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PillsControl"
             xmlns:commands="clr-namespace:PillsControl.Commands"
             xmlns:service="clr-namespace:PillsControl.Service"
             StartupUri="Windows/LoginWindow.xaml">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary>
					<commands:CommandImplementation x:Key="Commands"/>
					<service:UserProfileHandler x:Key="ProfileHandler"/>
				</ResourceDictionary>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
